{
    "arch": "arm",
    "llvm-target": "thumbv7m-none-eabi",
    "os": "none",
    "target-endian": "little",
    "target-pointer-width": "32",
    
    "linker": "arm-none-eabi-gcc",

    "cpu": "cortex-m3",
    "executables": true,
    "morestack": false,
    "no-compiler-rt": false,
    "relocation-model": "static",
    "custom-unwind-resume": true
}
